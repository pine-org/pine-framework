export ESHOP_HOST=0.0.0.0
export ESHOP_PORT=9091
export ENVIRONMENT=Development
export PROFILES=development,properties,i18n,h2,messaging,activemq-embedded,US
export DATABASE=eshop
export ACTIVEMQ_QUEUES=eshop,status

export ESHOP_VERSION=1.0.0-SNAPSHOT

mvn -f ./core-java8/pom.xml clean install -DskipTests=true
mvn -f ./core-spring/pom.xml clean install -DskipTests=true
mvn -f ./tutorial/pom.xml clean install -DskipTests=true

java -jar ./tutorial/eshop/rest-api/target/eshop-$ESHOP_VERSION.jar
