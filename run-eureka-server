export EUREKA_HOST=0.0.0.0
export EUREKA_PORT=8761
export HOSTNAME=$(hostname)
export PROFILES=development
export ENVIRONMENT=Development
export DATACENTER=Pine

export EUREKA_SERVER_VERSION=1.0.0-SNAPSHOT

mvn -f ./core-spring/discovery-server-eureka/pom.xml clean package
java -jar ./core-spring/discovery-server-eureka/target/discovery-server-eureka-$EUREKA_SERVER_VERSION.jar
